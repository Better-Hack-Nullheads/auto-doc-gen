{
    "name": "@auto-doc-gen/core",
    "version": "1.0.0",
    "description": "Simple NestJS controller and service analyzer",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "bin": {
        "auto-doc-gen": "dist/cli.js"
    },
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch",
        "start": "node dist/cli.js",
        "test": "jest",
        "lint": "eslint src/**/*.ts",
        "docs": "auto-doc-gen export src",
        "docs:analyze": "auto-doc-gen analyze src",
        "docs:save": "auto-doc-gen save src",
        "docs:ai": "auto-doc-gen ai src",
        "docs:config": "auto-doc-gen config"
    },
    "keywords": [
        "nestjs",
        "analyzer",
        "console",
        "simple",
        "documentation"
    ],
    "author": "Better-Hack-Nullheads",
    "license": "MIT",
    "dependencies": {
        "@ai-sdk/anthropic": "^1.0.0",
        "@ai-sdk/google": "^1.0.0",
        "@ai-sdk/openai": "^1.0.0",
        "ai": "^3.0.0",
        "chalk": "^4.1.2",
        "commander": "^11.0.0",
        "dotenv": "^17.2.3",
        "mongodb": "^6.0.0",
        "ts-morph": "^21.0.1",
        "zod": "^3.22.0"
    },
    "devDependencies": {
        "@nestjs/common": "^11.1.6",
        "@nestjs/core": "^11.1.6",
        "@types/jest": "^29.0.0",
        "@types/node": "^20.19.19",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "eslint": "^8.0.0",
        "jest": "^29.0.0",
        "typescript": "^5.0.0"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "peerDependencies": {
        "@nestjs/common": "^8.0.0 || ^9.0.0 || ^10.0.0 || ^11.0.0",
        "@nestjs/core": "^8.0.0 || ^9.0.0 || ^10.0.0 || ^11.0.0"
    },
    "peerDependenciesMeta": {
        "@nestjs/common": {
            "optional": true
        },
        "@nestjs/core": {
            "optional": true
        }
    }
}
